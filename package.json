{
  "name": "shopify-starter-theme",
  "version": "0.0.1",
  "description": "A Shopify starter theme using Theme Kit and Webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "deploy:live": "NODE_ENV=production webpack -p && theme deploy --env=prod-live --dir=theme --allow-live",
    "deploy:development": "NODE_ENV=production webpack -p && theme deploy --env=development --dir=theme",
    "deploy:theme:dev": "theme deploy --dir=theme --env=development",
    "watch:assets": "webpack --watch",
    "watch:theme:dev": "theme watch --notify=theme-ready.tmp --dir=theme --env=development",
    "prod:build": "NODE_ENV=production webpack --env=analyze -p",
    "staging:watch": "theme watch --notify=theme-ready.tmp --dir=theme --env=staging",
    "staging:deploy": "NODE_ENV=production webpack -p && theme deploy --env=staging --dir=theme"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@shopify/eslint-plugin": "^39.0.4",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^4.1.0",
    "read-yaml": "^1.1.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.3.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.31.2",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^3.1.0",
    "@shopify/theme-addresses": "^4.1.0",
    "@shopify/theme-cart": "^3.1.0",
    "@shopify/theme-currency": "^3.1.0",
    "@shopify/theme-images": "^3.1.0",
    "@shopify/theme-product": "^4.1.0",
    "@shopify/theme-product-form": "^4.1.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^10.4.0",
    "core-js": "^3.8.2",
    "cssnano": "^4.1.10",
    "enquire.js": "^2.1.6",
    "lazysizes": "^5.3.0",
    "postcss": "^8.3.11",
    "regenerator-runtime": "^0.13.7",
    "swiper": "^6.4.7",
    "tailwindcss": "^2.2.19"
  }
}
